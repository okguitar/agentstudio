{
  "name": "agentstudio-backend",
  "version": "0.2.0",
  "description": "Backend API for AgentStudio - AI-powered presentation editor with Claude integration",
  "main": "dist/index.js",
  "bin": {
    "agentstudio": "dist/bin/agentstudio.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "migrate-projects": "node scripts/migrate-projects.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.5",
    "@ai-sdk/openai": "^1.0.7",
    "@anthropic-ai/claude-agent-sdk": "^0.1.25",
    "@anthropic-ai/claude-code": "^2.0.10",
    "ai": "^5.0.22",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "undici": "^7.16.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.5.0",
    "@types/uuid": "^9.0.7",
    "@vitest/ui": "^2.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.46.1",
    "uuid": "^9.0.1",
    "vitest": "^2.1.0"
  },
  "author": "okguitar <okguitar@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/okguitar/agentstudio.git"
  },
  "homepage": "https://github.com/okguitar/agentstudio#readme",
  "bugs": {
    "url": "https://github.com/okguitar/agentstudio/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "studio",
    "editor",
    "presentation",
    "claude",
    "react",
    "typescript",
    "api"
  ],
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
